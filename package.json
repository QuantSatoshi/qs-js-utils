{
  "name": "qs-js-utils",
  "version": "0.0.29",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc && rimraf dist/test",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "prettier": "node ./node_modules/prettier/bin-prettier.js --parser=typescript --single-quote --trailing-comma=all --write --print-width=120 'src/**/*.ts'",
    "test": "npm run tsc && ava src/test/**/*.ts"
  },
  "ava": {
    "nodeArguments": [
      "--loader=ts-node/esm"
    ],
    "typescript": {
      "compile": false,
      "rewritePaths": {
        "src/": "dist/"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QuantSatoshi/qs-js-utils.git"
  },
  "author": "QS",
  "license": "ISC",
  "dependencies": {
    "byline": "^5.0.0"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/node": "^18.11.9",
    "ava": "^5.0.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
